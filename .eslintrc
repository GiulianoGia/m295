{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "max-lines-per-function": ["error", 30],
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "next"
            }
        ]
    },
    "plugins": ["prettier"]
}
